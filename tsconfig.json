{
	"exclude": ["node_modules"],
	"compilerOptions": {
		"outDir": "./dist/",
		"typeRoots": ["node_modules/@types"],
		"resolveJsonModule": true,
		"module": "CommonJS",
		"target": "ES2021",
		"esModuleInterop": true,
		"removeComments": true,
		"noUnusedParameters": true,
		"noUnusedLocals": true,
		"noFallthroughCasesInSwitch": true,
		"newLine": "lf",
		"inlineSourceMap": true
	}
}
