{
	"exclude": ["node_modules"],
	"compilerOptions": {
		"typeRoots": ["node_modules/@types"],
		"module": "esnext",
		"target": "ES2021",
		"esModuleInterop": true,
		"removeComments": true,
		"moduleResolution": "node",
		"noUnusedParameters": true,
		"noUnusedLocals": true,
		"noFallthroughCasesInSwitch": true,
		"newLine": "lf",
		"inlineSourceMap": true
	}
}
